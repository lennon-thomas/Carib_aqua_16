  issn = {0044-8486},
  doi = {10.1016/j.aquaculture.2008.03.030},
  abstract = {Methods and results of two larval rearing trials of cobia (Rachycentron canadum) are presented. These trials were designed to test the efficacy of protocols developed over several years of research in cobia larviculture at the UMEH. The protocols incorporate the use of probiotics and prophylaxis, minimize microalgae use, and include commercially available ingredients for live feed enrichment. During the trials, fertilized eggs were stocked at 400/L and incubated in 1000-L cylinder-conical tanks with flow-through seawater at 500\% daily exchange rate. Moderate aeration and pure oxygen were used to maintain dissolved oxygen concentrations above saturation (6.5~mg/L at 26~$^\circ$C). Hatching occurred at 22\textendash{}24~h post fertilization. Two day-post-hatch (dph) yolk-sac larvae were stocked in four 12,000-L cylinder-conical tanks at 5 and 10 larvae/L. Beginning on 3~dph, larvae were fed microalgae (Isochyrsis galbana C-strain) at low concentrations (5\textendash{}10,000~cell/ml) and enriched rotifers (Brachionus plicatilis) at 3\textendash{}5/mL through 9~dph. Beginning on 7~dph, enriched Artemia (Artemia franciscana GSL Strain) nauplii were fed to larvae at 0.1\textendash{}1/mL. Cobia larvae were reared at water temperatures ranging from 24.3 to 31.8~$^\circ$C. Water quality parameters were within normal ranges for seawater: salinity 26\textendash{}34~ppt, pH 7.92\textendash{}8.16, and NH3 \&lt; 0.18~mg/L. Vigorous aeration and supplemental oxygen were used at all times during both larval rearing trials to maintain adequate water movement and levels of dissolved oxygen (DO) (7.0\textendash{}9.0~mg/L). Water was filtered down to 10~$\mu$m using standard sand filters filled with broken glass media and bag filters prior to entering the tanks. Daily water exchange rates in the tanks ranged from 100\% at 3~dph to 500\% from 17~dph onwards. Between 20 and 22~dph, all post-larvae were fully weaned onto dry starting diets. Survival rates of post-larvae measuring 1.5\textendash{}2.0~cm SL and weighing 0.5~g at 20\textendash{}22~dph were estimated to be $\geq$ 50\%. Further mortality during the nursery stage to 3\textendash{}5~cm and 1\textendash{}3~g fingerlings prior to shipping at 27~dph brought the overall survival rate to an average of 25.7\%. Survival rates of fingerlings cultured in tanks initially stocked at lower densities (5~larvae/L) was significantly higher (P = 0.0078). From 15~dph, post-larvae and fingerlings were daily graded by size with large individuals singled out and stocked into another tank. These trials generated 125,328 fingerlings in four tanks in just two months, levels of production that could sustain a commercial operation and indicate that cobia aquaculture can be viable in the Americas.},
  timestamp = {2017-05-17T23:07:39Z},
  number = {1\textendash{}4},
  urldate = {2017-05-17},
  journal = {Aquaculture},
  author = {Benetti, Daniel D. and Sardenberg, Bruno and Welch, Aaron and Hoenig, Ronald and Orhun, M. Refik and Zink, Ian},
  month = sep,
  year = {2008},
  keywords = {cobia,Fingerling production,Larval culture,Rachycentron canadum},
  pages = {22--27},
  file = {ScienceDirect Full Text PDF:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/KAU32X6Q/Benetti et al. - 2008 - Intensive larval husbandry and fingerling producti.pdf:application/pdf;ScienceDirect Snapshot:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/V96GA23A/S0044848608002007.html:text/html},
  groups = {carib\_aqua\_16}
}

@article{Benetti2008c,
