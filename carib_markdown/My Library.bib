  year = {2013},
  keywords = {Freshwater \& Marine Ecology,Marine \& Freshwater Sciences,Microbiology,Oceanography,Zoology},
  pages = {3005--3014},
  file = {Full Text PDF:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/XNEQQCC6/Schaefer and Fuller - 2013 - Simultaneous behavior of skipjack (Katsuwonus pela.pdf:application/pdf;Snapshot:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/64NF4TN9/s00227-013-2290-9.html:text/html},
