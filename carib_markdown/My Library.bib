@article{Nagelkerken2005a,
  title = {Changes in {{Coral Reef Communities}} and an {{Associated Reef Fish Species}}, {{Cephalopholis}} Cruentata ({{Lac{\'e}p{\`e}de}}), {{After}} 30 Years on {{Cura{\c c}ao}} ({{Netherlands Antilles}})},
  volume = {549},
  issn = {0018-8158, 1573-5117},
  doi = {10.1007/s10750-005-5448-8},
  abstract = {Using the same methodology and identical sites, we repeat a study dating from 1973 and quantify cover of hard coral species, soft corals, sponges, hard substratum and soft substratum, and density of a commercially important reef fish species, the graysby Cephalopholis cruentata, along a depth-gradient of 3\textendash{}36 m on the coral reefs of Cura{\c c}ao. The objective was to determine the multi-decade change in benthic coral reef cover and structural complexity, and their effect on densities of an associated reef fish species. Total hard coral cover decreased on average from 52\% in 1973 to 22\% in 2003, representing a relative decline of 58\%. During this time span, the cover of hard substratum increased considerably (from 11 to 58\%), as did that of soft corals (from 0.1 to 2.2\%), whereas the cover of sponges showed no significant change. Relative decline of hard coral cover and of reef complexity was greatest in shallow waters (near the coast), which is indicative of a combination of anthropogenic influences from shore and recent storm damage. Cover of main reef builder coral species (Agaricia spp., Siderastrea siderea, Montastrea annularis) decreased more than that of other species, and resulted in a significant decrease in reef complexity. Although density of C. cruentata was highly correlated to cover of Montastrea and Agaricia in 1973, the loss of coral cover did not show any effect on the total density of C. cruentata in 2003. However, C. cruentata showed a clear shift in density distribution from shallow water in 1973 to deep water in 2003. It can be concluded that the reefs of Cura{\c c}ao have degraded considerably in the last three decades, but that this has had no major effect on the population size of one commercially important coral-associated fish species.},
  language = {en},
  timestamp = {2015-05-26T22:33:47Z},
  number = {1},
  urldate = {2015-05-26},
  journal = {Hydrobiologia},
  author = {Nagelkerken, I. and Vermonden, K. and Moraes, O. C. C. and Debrot, A. O. and Nagelkerken, W. P.},
  month = oct,
  year = {2005},
  keywords = {Caribbean,coral reef communities,coral reef complexity,coral reef degradation,Ecology,fish population structure,Hydrobiology,Serranidae},
  pages = {145--154},
  file = {Full Text PDF:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/A936BG7V/Nagelkerken et al. - 2005 - Changes in Coral Reef Communities and an Associate.pdf:application/pdf;Snapshot:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/F32WTKDB/s10750-005-5448-8.html:text/html},
  groups = {Curacao lit review}
}

@phdthesis{Wijekoon2012,
  type = {doctoral},
  title = {Effect of Water Temperature and Diet on Cell Membrane Fluidity and Fatty Acid Composition of Muscle, Liver, Gill and Intestine Mucosa of Adult and Juvenile Steelhead Trout, {{Oncorhynchus}} Mykiss},
  abstract = {Chapter I highlights the significant role of lipids in fish as a source of metabolic energy for growth, reproduction, movement and health. Dietary lipids also influence the fatty acid composition in various tissues including cellular membranes altering composition and function. Fish oil (FO) derived from capture fisheries is the main source of lipid in aquaculture feed to provide the essential fatty acids for  maricultured fish. However, limitations in FO availability have prompted the investigation of the use of vegetable oil and animal fat as substitutes for fish oil and are investigated in this study. -- Chapter 2 reports the results of investigation of the effect of FO substituted commercial and experimental diets on muscle lipid class and fatty acid composition in adult and juvenile steelhead trout (Oncorhynchus mykiss). Study (i) compared herring oil (HE) diets with those containing medium levels of omega 3 fatty acids (M-$\omega$3) and lower $\omega$3 levels (L-$\omega$3), and study (ii) compared HE with sunflower oil (SF) and flax seed oil (FLX).    The results of both studies indicate the possibility of substitution of FO without compromising growth, feed efficiency or health once the minimal essential fatty acid requirements are met in the diet. HE diets with higher $\omega$3 levels (H-$\omega$3) in both studies significantly increased the $\omega$3 highly unsaturated fatty acid composition in muscle tissue with a corresponding decrease in plant terrestrial fatty acid composition (p $\leq$ 0.045). However, fish fed M-$\omega$3, L-$\omega$3, SF and FLX diets selectively accumulated DHA (docosahexaenoic acid) in muscle tissue minimizing the dietary differences in both adult and juvenile fish. All M-$\omega$3, L-$\omega$3 and SF diets were rich in linoleic acid and increased the $\omega$6 composition in muscle tissue altering the $\omega$6:$\omega$3 ratio. In contrast, both HE and FLX diets had similar $\sum\omega$3 and $\omega$6:$\omega$3 ratios due to the high availability of linolenic acid in FLX feed. Triacylglycerols were the main neutral lipid stored in muscle tissue (above 80 - 90\% total lipid). Changes in water temperature had minimal effects on muscle lipid composition in both adult and juvenile fish. -- Chapter 3 investigated the effect of H-$\omega$3, M-$\omega$3 and L-$\omega$3 diets and changes in water temperature on liver membrane fluidity, lipid class and fatty acid composition in adult fish. Fish were sampled from the initial (l3.5$^\circ$C), middle (18.0$^\circ$C) and final (13.5$^\circ$C) experimental temperatures used in study (i). The membrane fluidity was determined by measuring the CHâ‚‚ symmetric stretching bands in Raman spectroscopy at 2850 cmâ»$^1$. The stretching frequencies were obtained by subjecting the liver lipid to a temperature range of 2 to 30$^\circ$C in increments of 4$^\circ$C. The physical properties of the liver membrane lipid in fish fed H-$\omega$3 diets was the least influenced by quickly adapting to either  an increase or decrease in water temperature. The membrane fluidity of fish fed M-$\omega$3 diets at 18.0$^\circ$C, was similar to fish fed H-$\omega$3. The liver membranes of L-$\omega$3 diet fed fish sampled at both 18.0 and 13.5$^\circ$C became increasingly more fluid than both H-$\omega$3 and M-$\omega$3 fed fish during the Raman trial. The fatty acid proportion of 20:3$\omega$6 and 20:4$\omega$6 at 18.0$^\circ$C and 20:3$\omega$6 at 13.5$^\circ$C (final) was significantly higher in fish fed L-$\omega$3 diets and may have influenced the increased fluidity in liver cell membranes. Both M-$\omega$3 and H-$\omega$3 fed fish have the capability to withstand a broader temperature range without undergoing complete phase separation of membrane lipids. -- Chapter 4 evaluated the lipid composition of gills, intestine mucosa and liver tissue of juvenile steelhead trout using HE, SF and FLX diets, during a water temperature increase from 8 to 18.0$^\circ$C. The lipid class composition of gill tissue was not affected by diet or  temperature. Both SF and FLX diets significantly increased the terrestrial fatty acid composition and $\omega$6:$\omega$3 ratio in gills of juvenile steelhead trout. The variation in diets also influenced the storage and selection of fatty acid substrates for $\beta$-oxidation in juvenile steelhead trout. Fish  fed HE diets deposited a higher amount of saturated fatty acids in the liver while monounsaturated fatty acids were significantly higher in both SF and FLX diet fed fish. Both SF and FLX diets stimulated chain elongation and desaturation in steelhead juveniles. The SF diet stimulated the production of arachidonic acid while eicosapentaenoic acid production was promoted by the FLX diet.},
  timestamp = {2015-04-02T22:22:33Z},
  urldate = {2015-04-02},
  school = {Memorial University of Newfoundland},
  author = {Wijekoon, Manjusri Pandula Atapattu},
  year = {2012},
  file = {Full Text PDF:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/PVN6UB97/Wijekoon - 2012 - Effect of water temperature and diet on cell membr.pdf:application/pdf;Snapshot:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/KWPA2A9D/2412.html:text/html}
}

@article{Kuenen1995a,
  title = {A Quantitative Study of the Seagrass and Algal Meadows of the {{Spaanse Water}}, {{Cura{\c c}ao}}, the {{Netherlands Antilles}}},
  volume = {51},
  issn = {0304-3770},
  doi = {10.1016/0304-3770(95)00478-I},
  abstract = {The Spaanse Water is a relatively turbid, 3.19 km2 inland bay of virtually oceanic salinities and contains the largest seagrass, algal and mangrove areas of the Cura{\c c}ao Underwater Park. During 1989 and 1990, a quantitative community assessment of the larger attached flora and fauna of the seagrass and algal meadows of the bay was conducted at 151 6 m2 stations using a quadrat sampling technique.

A total of 13 different assemblages were distinguished. Shallow assemblages were dominated by Thalassia testudinum and Halimeda opuntia. As depth increased and light levels decreased, Thalassia gave way to increased coverages of especially H. opuntia, H. incrassata, Cladophora sp. and Caulerpa verticillata. In areas with significant availability of hard substrate an assemblage characterised (though not dominated) by corals was found at depths of 0\textendash{}2 m, while sponges were concentrated at depths of about 4 m. The richest assemblages were found in shallow areas with high light levels and where a mix of both hard and soft substrate occurred. Assemblages with the lowest species richness were typically associated with low light intensities, soupy muds or homogeneous sandy sediments of high grain size.},
  timestamp = {2015-04-13T20:50:21Z},
  number = {3\textendash{}4},
  urldate = {2015-04-13},
  journal = {Aquatic Botany},
  author = {Kuenen, M. M. C. E. and Debrot, A. O.},
  month = aug,
  year = {1995},
  keywords = {Algae,Assemblages,Benthic,Communities,Seagrass,Tropical},
  pages = {291--310},
  file = {ScienceDirect Full Text PDF:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/XUFK98MW/Kuenen and Debrot - 1995 - A quantitative study of the seagrass and algal mea.pdf:application/pdf;ScienceDirect Snapshot:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/3NJZFNZM/030437709500478I.html:text/html},
  groups = {in paper}
}

