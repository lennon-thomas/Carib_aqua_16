  urldate = {2017-04-21},
  journal = {Canadian Journal of Fisheries and Aquatic Sciences},
  author = {Martell, Steven J.D. and Essington, Timothy E and Lessard, Bob and Kitchell, James F and Walters, Carl J and Boggs, Christofer H},
  month = jun,
