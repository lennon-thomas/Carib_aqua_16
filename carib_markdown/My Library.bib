@article{Gruss2011,
  title = {Consequences of Adult and Juvenile Movement for Marine Protected Areas},
  volume = {144},
  issn = {0006-3207},
  doi = {10.1016/j.biocon.2010.12.015},
  abstract = {Adult and juvenile mobility has a considerable influence on the functioning of marine protected areas. It is recognized that adult and juvenile movement reduces the core benefits of protected areas, namely protecting the full age\textendash{}structure of marine populations, while at the same time perhaps improving fisheries yield over the no-reserve situation through export of individuals from protected areas. Nevertheless, the study of the consequences of movement on protected area functioning is unbalanced. Significant attention has been paid to the influence of certain movement patterns, such as diffusive movement and home ranges, while the impacts of others, such as density-dependent movements and ontogenetic migrations, have been relatively ignored. Here we review the diversity of density-independent and density-dependent movement patterns, as well as what is currently known about their consequences for the conservation and fisheries effects of marine protected areas. We highlight a number of `partially addressed' issues in marine protected area research, such as the effects of reserves targeting specific life phases, and a number of essentially unstudied issues, such as density-dependent movements, nomadism, ontogenetic migrations, behavioral polymorphism and `dynamic' reserves that adjust location as a realtime response to habitat changes. Assessing these issues will be essential to creating effective marine protected area networks for mobile species and accurately assessing reserve impacts on these species.},
  timestamp = {2015-04-07T20:37:45Z},
  number = {2},
  urldate = {2015-04-07},
  journal = {Biological Conservation},
  author = {Gr{\"u}ss, Arnaud and Kaplan, David M. and Gu{\'e}nette, Sylvie and Roberts, Callum M. and Botsford, Louis W.},
  month = feb,
  year = {2011},
  keywords = {Density-dependent movement,Density-independent movement,Dynamic MPAs,Marine protected areas (MPAs),spillover,Targeted MPAs},
  pages = {692--702},
  file = {ScienceDirect Full Text PDF:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/8MPDXNTM/GrÃ¼ss et al. - 2011 - Consequences of adult and juvenile movement for ma.pdf:application/pdf;ScienceDirect Snapshot:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/HUW4TTR2/S0006320710005240.html:text/html},
