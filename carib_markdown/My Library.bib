}

@article{White2008,
  title = {Marine Reserve Effects on Fishery Profit},
  volume = {11},
  copyright = {\textcopyright{} 2008 Blackwell Publishing Ltd/CNRS},
  issn = {1461-0248},
  doi = {10.1111/j.1461-0248.2007.01151.x},
  abstract = {Some studies suggest that fishery yields can be higher with reserves than under conventional management. However, the economic performance of fisheries depends on economic profit, not fish yield. The predictions of higher yields with reserves rely on intensive fishing pressures between reserves; the exorbitant costs of harvesting low-density populations erode profits. We incorporated this effect into a bioeconomic model to evaluate the economic performance of reserve-based management. Our results indicate that reserves can still benefit fisheries, even those targeting species that are expensive to harvest. However, in contrast to studies focused on yield, only a moderate proportion of the coast in reserves (with moderate harvest pressures outside reserves) is required to maximize profit. Furthermore, reserve area and harvest intensity can be traded off with little impact on profits, allowing for management flexibility while still providing higher profit than attainable under conventional management.},
  language = {en},
  timestamp = {2015-04-03T23:13:55Z},
  number = {4},
  urldate = {2015-04-03},
  journal = {Ecology Letters},
  author = {White, Crow and Kendall, Bruce E. and Gaines, Steven and Siegel, David A. and Costello, Christopher},
  month = apr,
  year = {2008},
  keywords = {bioeconomics,density dependence,fishery profit,marine reserves,stock effect},
  pages = {370--379},
  file = {Full Text PDF:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/9IJVXWFV/White et al. - 2008 - Marine reserve effects on fishery profit.pdf:application/pdf;Snapshot:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/CPGAWCKC/full.html:text/html}
}

@article{Dahlgren2000,
  title = {Designing a {{Dry Tortugas Ecological Reserve}}: {{How}} Big Is Big Enough? \ldots{}to Do What?},
  volume = {66},
  shorttitle = {Designing a {{Dry Tortugas Ecological Reserve}}},
  abstract = {A review of the global experience with no-take marine reserves strongly suggests that they are important tools for marine conservation and fishery management, capable of providing benefits in the form of ecosystem protection, improved fishery yields, expanded understanding of marine systems, and improved nonconsumptive opportunities. The degree to which a reserve will provide certain benefits or achieve specific goals will vary with species, depending on life-history characteristics and various aspects of reserve design. The Florida Keys National Marine Sanctuary management plan created a network of no-take reserves encompassing just 0.5\% of the sanctuary's total area but provided for the creation of a Dry Tortugas Ecological Reserve, an additional marine reserve in and adjacent to the Dry Tortugas region of the sanctuary. The existing reserves are small because they were intended to provide only limited fishery benefits, but additional fishery benefits are being considered among the objectives of a Dry Tortugas reserve. How big must such a reserve be to provide specific benefits, and how can one predict this size without having one? We use a simple model based on the percent of virgin biomass (\%B0) in fished and unfished areas to provide managers with a quick and easy way of estimating the reserve size required to meet specific management objectives. Analyses of \%B0 for populations in fished and reserve areas suggests that a Dry Tortugas reserve encompassing at least 30\textendash{}40\% of the region is required to elevate all stocks from current levels to overfishing-threshold \%B0 levels, but smaller reserves might be used to complement conventional fisheries-management practices as a buffer against some level of overfishing and insurance against complete stock collapse.},
  timestamp = {2015-05-07T23:54:22Z},
  number = {3},
  journal = {Bulletin of Marine Science},
  author = {Dahlgren, Craig P. and Sobel, Jack},
  month = may,
  year = {2000},
  pages = {707--719},
  file = {IngentaConnect Full Text PDF:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/99BT4HK4/Dahlgren and Sobel - 2000 - Designing a Dry Tortugas Ecological Reserve How b.pdf:application/pdf},
  groups = {MSP with Ren}
}

