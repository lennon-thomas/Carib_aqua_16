@article{Bak2005a,
  title = {Coral Reef Crisis in Deep and Shallow Reefs: 30 Years of Constancy and Change in Reefs of {{Curacao}} and {{Bonaire}}},
  volume = {24},
  issn = {0722-4028, 1432-0975},
  shorttitle = {Coral Reef Crisis in Deep and Shallow Reefs},
  doi = {10.1007/s00338-005-0009-1},
  abstract = {Coral reefs are thought to be in worldwide decline but available data are practically limited to reefs shallower than 25 m. Zooxanthellate coral communities in deep reefs (30\textendash{}40 m) are relatively unstudied. Our question is: what is happening in deep reefs in terms of coral cover and coral mortality? We compare changes in species composition, coral mortality, and coral cover at Caribbean (Curacao and Bonaire) deep (30\textendash{}40 m) and shallow reefs (10\textendash{}20 m) using long-term (1973\textendash{}2002) data from permanent photo quadrats. About 20 zooxanthellate coral species are common in the deep-reef communities, dominated by Agaricia sp., with coral cover up to 60\%. In contrast with shallow reefs, there is no decrease in coral cover or number of coral colonies in deep reefs over the last 30 years. In deep reefs, non-agaricid species are decreasing but agaricid domination will be interrupted by natural catastrophic mortality such as deep coral bleaching and storms. Temperature is a vastly fluctuating variable in the deep-reef environment with extremely low temperatures possibly related to deep-reef bleaching.},
  language = {en},
  timestamp = {2015-05-26T22:34:23Z},
