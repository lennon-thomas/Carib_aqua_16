@article{Govers2014d,
  title = {Seagrasses as Indicators for Coastal Trace Metal Pollution: {{A}} Global Meta-Analysis Serving as a Benchmark, and a {{Caribbean}} Case Study},
  volume = {195},
  issn = {0269-7491},
  shorttitle = {Seagrasses as Indicators for Coastal Trace Metal Pollution},
  doi = {10.1016/j.envpol.2014.08.028},
  abstract = {Seagrass beds are highly productive coastal ecosystems providing a large array of ecosystem services including fisheries and carbon sequestration. As seagrasses are known to be highly sensitive to anthropogenic forcing, we evaluated the use of trace metal concentrations in seagrasses as bioindicators for trace metal pollution of coastal regions at both global and local scale. We carried out a meta-analysis based on literature data to provide a global benchmark list for trace metal accumulation in seagrasses, which was lacking in literature. We subsequently carried out a case study at the Caribbean islands of Cura{\c c}ao and Bonaire to test for local-scale differences in trace metal concentrations in seagrasses, and internal metal allocation. The benchmark and local study show that trace metal concentrations in seagrass leaves, regardless of the species, can vary over a 100\textendash{}1000-fold range, and are related to the level of anthropogenic pressure, making seagrasses highly valuable indicators.},
  timestamp = {2015-04-13T20:37:47Z},
  urldate = {2015-04-13},
  journal = {Environmental Pollution},
  author = {Govers, Laura L. and Lamers, Leon P. M. and Bouma, Tjeerd J. and Eygensteyn, Jelle and {de Brouwer}, Jan H. F. and Hendriks, A. Jan and Huijbers, Chantal M. and {van Katwijk}, Marieke M.},
  month = dec,
  year = {2014},
  keywords = {Bioindicator,Ecosystem service,Heavy metal,Review,Thalassia testudinum},
  pages = {210--217},
  file = {ScienceDirect Full Text PDF:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/GGNJEF4B/Govers et al. - 2014 - Seagrasses as indicators for coastal trace metal p.pdf:application/pdf;ScienceDirect Snapshot:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/5KTVJEKG/S0269749114003777.html:text/html}
