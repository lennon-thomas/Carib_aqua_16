  keywords = {Bleaching,Coral mortality,Coral reef temperature,Deep coral reef,Freshwater \& Marine Ecology,Global change,Long-term monitoring,Oceanography},
  pages = {475--479},
  file = {Full Text PDF:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/AGCQQ3I5/Bak et al. - 2005 - Coral reef crisis in deep and shallow reefs 30 ye.pdf:application/pdf;Snapshot:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/8BDDCHQ8/s00338-005-0009-1.html:text/html},
