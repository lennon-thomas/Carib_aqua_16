@article{Stewart2005,
  title = {Efficiency, Costs and Trade-Offs in Marine Reserve System Design},
  volume = {10},
  issn = {1420-2026, 1573-2967},
  doi = {10.1007/s10666-005-9001-y},
  abstract = {With marine biodiversity conservation the primary goal for reserve planning initiatives, a site's conservation potential is typically evaluated on the basis of the biological and physical features it contains. By comparison, socio-economic information is seldom a formal consideration of the reserve system design problem and generally limited to an assessment of threats, vulnerability or compatibility with surrounding uses. This is perhaps surprising given broad recognition that the success of reserve establishment is highly dependent on widespread stakeholder and community support. Using information on the spatial distribution and intensity of commercial rock lobster catch in South Australia, we demonstrate the capacity of mathematical reserve selection procedures to integrate socio-economic and biophysical information for marine reserve system design. Analyses of trade-offs highlight the opportunities to design representative, efficient and practical marine reserve systems that minimise potential loss to commercial users. We found that the objective of minimising the areal extent of the reserve system was barely compromised by incorporating economic design constraints. With a small increase in area ($<$3\%) and boundary length ($<$10\%), the economic impact of marine reserves on the commercial rock lobster fishery was reduced by more than a third. We considered also how a reserve planner might prioritise conservation areas using information on a planning units selection frequency. We found that selection frequencies alone were not a reliable guide for the selection of marine reserve systems, but could be used with approaches such as summed irreplaceability to direct conservation effort for efficient marine reserve design.},
  language = {en},
  timestamp = {2015-05-13T18:20:25Z},
  number = {3},
  urldate = {2015-05-13},
  journal = {Environmental Modeling \& Assessment},
  author = {Stewart, Romola R. and Possingham, Hugh P.},
  month = nov,
  year = {2005},
  keywords = {Applications of Mathematics,bioeconomics,conservation planning,decision theory,Environment; general,irreplaceability,marine reserves,Mathematical Modeling and Industrial Mathematics,reserve selection,trade-offs},
  pages = {203--213},
  file = {Full Text PDF:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/79V3HHVB/Stewart and Possingham - 2005 - Efficiency, costs and trade-offs in marine reserve.pdf:application/pdf;Snapshot:/Users/lennonthomas/Library/Application Support/Zotero/Profiles/26g9pv7t.default/zotero/storage/MXGKB5RB/Stewart and Possingham - 2005 - Efficiency, costs and trade-offs in marine reserve.html:text/html}
}

