@article{Bak1995a,
  title = {Long-{{Term Change}} in {{Coral Communities Along Depth Gradients Over Leeward Reefs}} in the {{Netherlands Antilles}}},
  volume = {56},
  abstract = {Reef slope coral communities were surveyed for long-term (20 years) changes in scleractinian coral cover, numbers of coral colonies and species richness, over the time intervals between the years 1973, 1983 and 1992. We compare such long-term structural changes in the communities at depths of 10, 20, 30 and 40 m. Our data are based on series of photographic records of permanent quadrats, a total of 36 m2 reef bottom at each depth, along four transects on the leeward coasts of the islands of Cura{\c c}ao and Bonaire. We summarize the changes in the permanent quadrats over time to demonstrate the main trends in the data set and, to understand the significance of the data for the reef community, test the results as effects of time and depth using mixed model ANOVA's. Changes in numbers of coral colonies and coral cover were a function of depth. Number of coral colonies decreased significantly at depths of 10, 20 and 30 m, but not at 40 m. Coral cover decreased significantly at 10 and 20 m, but not at 30 and 40 m. Diversity (species richness) decreased through the years independent of depth. There were no consistent differences between the two 10-year time-intervals. These results confirm earlier observations of coral mortality and spatial mobility which showed the deep reef (30, 40 m) as a much more constant environment than the relatively disturbed shallower reef (10, 20 m).},
  timestamp = {2015-05-13T05:49:46Z},
  number = {2},
  journal = {Bulletin of Marine Science},
  author = {Bak, R. P. M. and Nieuwland, G.},
  month = mar,
  year = {1995},
  pages = {609--619}
}

